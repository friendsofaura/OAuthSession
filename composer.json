{
    "name": "friendsofaura/oauthsession",
    "type": "library",
    "description": "Aura session storage in Lusitanian PHP oauth library",
    "keywords": [ "session", "oauth", "aura" ],
    "homepage": "https://github.com/friendsofaura/OAuthSession",
    "license": "BSD-2-Clause",
    "authors": [
        {
            "name": "Friends of Aura Contributors",
            "homepage": "https://github.com/friendsofaura/OAuthSession/contributors"
        }
    ],
    "require": {
        "aura/session":"~1.0",        
        "lusitanian/oauth": "dev-master"
    },
    "autoload": {
        "classmap": ["src/"]
    },
    "extra": {
        "aura": {
            "type": "kernel"
        },
        "branch-alias": {
            "dev-develop": "1.0.x-dev"
        }
    }
}
